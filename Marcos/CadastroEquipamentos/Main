    public static void main(String[] args) {
        // Cria o cadastro que vai gerenciar os equipamentos
        CadastroEquipamentos cadastro = new CadastroEquipamentos();

        // Aqui você poderia pedir ao usuário para cadastrar equipamentos via teclado.
        // Como exemplo, vamos só listar (a lista estará vazia)
        cadastro.listarEquipamentos();
    }
}
